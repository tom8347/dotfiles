
! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

! -------------XTerm------------------------
xterm*faceName: DejaVu Sans Mono for Powerline
xterm*faceSize: 13

xterm*selection_foreground: #282c34
xterm*selection_background: #979eab

xterm*backarrowKey: False
xterm*selectToClipboard: True

xterm.vt100.translations: #override \n\
    Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
    Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

! ---------------urxvt---------------------------
URxvt.font: xft:DejaVu Sans Mono for Powerline:size=13
URxvt.boldFont: xft:DejaVu Sans Mono for Powerline:bold:size=13
URxvt*scrollBar: False
